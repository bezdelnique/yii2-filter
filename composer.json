{
    "name": "bezdelnique/yii2-filter",
    "description": "Yii2 filter library",
    "type": "yii2-extension",
    "keywords": [
        "yii2",
        "extension"
    ],
    "license": "MIT License",
    "authors": [
        {
            "name": "Vitaly Belenky",
            "email": "vitaly.belenky@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "bezdelnique\\yii2filter\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url":  "https://github.com/bezdelnique/yii2-app"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "bezdelnique/yii2-app": "dev-master"
    },
    "require-dev": {
        "codeception/verify": "^0.4.0",
        "yiisoft/yii2": "^2.0"
    }
}
